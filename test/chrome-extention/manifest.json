{
	"name": "FBParser Tester",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Tester for fbparser library",
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "FBParser"
    },
	"background": {
	    "scripts": ["js/background.js"]
	},
	"permissions": [
		"cookies",
		"tabs",
		"notifications",
		"contentSettings",
		"http://*/",
		"http://*/*",
		"https://*/",
		"https://*/*",
		"<all_urls>"
	]
}
